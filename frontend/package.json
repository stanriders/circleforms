    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "whatwg-fetch": "^3.6.2"
  }
