{
  "name": "circleforms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "e2e:dev": "npx playwright test --config=playwright.dev.config.ts",
    "e2e:codegen": "npx playwright codegen --save-storage=auth.json",
    "lint": "next lint -- --fix",
    "ts-lint": "tsc --pretty --noEmit",
    "format": "prettier --write ./src/*",
    "generate-api-client": "openapi-generator-cli generate -i http://localhost/swagger/v1/swagger.json -o ./openapi -g typescript-fetch --additional-properties=supportsES6=true,modelPropertyNaming='original'",
    "tsignore-openapi": "bash ./tsignore.sh",
    "eslint": "eslint --fix .",
    "lefthook-install": "cd .. && npx @evilmartians/lefthook install -a"
  },
  "dependencies": {
    "@bbob/core": "^2.8.1",
    "@bbob/html": "^2.8.1",
    "@bbob/preset-html5": "^2.8.1",
    "@hookform/resolvers": "^2.9.6",
    "@mantine/core": "^4.2.7",
    "@mantine/dates": "^4.2.7",
    "@mantine/hooks": "^4.2.7",
    "@mantine/modals": "^4.2.7",
    "@reach/listbox": "^0.17.0",
    "@reach/menu-button": "^0.17.0",
    "@reach/tabs": "^0.17.0",
    "@reach/visually-hidden": "^0.17.0",
    "autosize": "^5.0.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "flag-icons": "^6.6.4",
    "install": "^0.13.0",
    "js-cookie": "^3.0.1",
    "next": "^12.2.3",
    "next-intl": "^2.7.4",
    "next-superjson": "^0.0.4",
    "nextjs-progressbar": "^0.0.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.33.1",
    "react-hot-toast": "^2.3.0",
    "react-icons": "^4.4.0",
    "react-inlinesvg": "^3.0.0",
    "react-query": "^3.39.2",
    "react-select": "^5.4.0",
    "react-switch": "^7.0.0",
    "timeago.js": "^4.0.2",
    "ts-debounce": "^4.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.0.5",
    "@hookform/devtools": "^4.2.2",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@playwright/test": "^1.24.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.3.0",
    "@types/autosize": "^4.0.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/typescript-estree": "^5.31.0",
    "@whiterussianstudio/tailwind-easing": "^1.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-tailwindcss": "^3.6.0",
